algoritmo "semnome"
// Função :
// Autor :
// Data : 22/10/2024
// Seção de Declarações 
var

   numero, soma: inteiro
   i: inteiro
   
inicio
   soma <- 0

   // Laço para somar 5 números
   para i de 1 ate 5 faca
      escreva("Digite o ", i, "º número: ")
      leia(numero)
      soma <- soma + numero
   fimpara

   escreva("A soma dos números é: ", soma)
fimalgoritmo

